#letsGo
 
from tkinter import*
import tkinter.messagebox
#import stdDatabase

class Student:

    def __init__(self,root):
        self.root=root        
        self.root.title("Student Details SDS")
        self.root.geometry("1350x750+0+0")
        self.root.config(bg="cadet blue")

        StdName= StringVar()
        FirstName= StringVar()
        SurName= StringVar()
        Board= StringVar()
        Age= StringVar()
        DoB= StringVar()
        Stream= StringVar()
        Board= StringVar()

        ##FRAMES
        MainFrame=Frame(self.root,bg="cadet blue")
        MainFrame.grid()
        
        TitFrame=Frame(MainFrame,bd=2,padx=54,pady=8,bg="Ghost White",relief=RIDGE)
        TitFrame.pack(side=TOP)

        self.lblTit=Label(Titframe,font=('arial',47,'bold'),text="SDS",bg="Ghost White")
        self.lblTit.grid()

        ButtonFrame=Frame(MainFrame,bd=1,width=1350,height=400,padx=20,pady=20,relief=RIDGE,bg='cadet blue')
        ButtonFrame.pack(side=BOTTOM)

        ButtonFrame=Frame(MainFrame,bd=1,width=1350,height=400,padx=20,pady=20,relief=RIDGE,bg='cadet blue')
        ButtonFrame.pack(side=BOTTOM)

        



if __name__=='__main__':
                         root=Tk()
                         application=Student(root)
                         root.mainloop()


























                         
